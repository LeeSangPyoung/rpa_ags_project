<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rpa.mapper.RpaStatusMapper">
    <select id="getStatusCbx" resultMap="RpaComboboxResultMap.RpaCombobox">
        SELECT ID AS VALUE,
        NAME AS LABEL
        FROM RPA_STATUS
